r\n        {\r\n          email: email,\r\n          password: password,\r\n        }\r\n      );\r\n      const responseUser = await axios.get(\r\n        \"https://git.heroku.com/limitless-shore-94245.git/api/v1/users\" ,\r\n        {\r\n          params: {\r\n            email: email,\r\n          },\r\n        }\r\n      );\r\n      commit(\"auth\", responseLogin.data.auth);\r\n      commit(\"user\", responseUser.data.data[0]);\r\n      router.replace(\"/\");\r\n    },\r\n    logout({ commit }) {\r\n      axios\r\n        .post(\"https://git.heroku.com/limitless-shore-94245.git/api/v1/users/logout\", {\r\n          auth: this.state.auth,\r\n        })\r\n        .then((response) => {\r\n          console.log(response);\r\n          commit(\"logout\", response.data.auth);\r\n          router.replace(\"/\");\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n});"]}]}