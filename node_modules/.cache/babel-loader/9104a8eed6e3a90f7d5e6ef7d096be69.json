AAA,CAAA;AACA,kBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,MAAA,EAAA,KAAA,GACA,KAAA,CAAA,KAAA,GAAA,IADA;AAEA,iBAbA,CADA;;AAAA;AAAA,sBAkBA,KAAA,CAAA,KAAA,KAAA,IAAA,IAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,KAAA,IAlBA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAqBA,KAAA,CACA,IADA,CACA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,GAAA,4BADA,EACA;AACA,kBAAA,IAAA,EAAA,KAAA,CAAA,IADA;AAEA,kBAAA,KAAA,EAAA,KAAA,CAAA,KAFA;AAGA,kBAAA,QAAA,EAAA,KAAA,CAAA;AAHA,iBADA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,kBAAA,KAAA,CAAA,KAAA,CAAA;AACA,iBARA,CArBA;;AAAA;AAAA;AAAA,uBAgCA,KAAA,CAAA;AACA,kBAAA,MAAA,EAAA,KADA;AAEA,kBAAA,GAAA,EAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,GAAA,eAFA;AAGA,kBAAA,MAAA,EAAA;AACA,oBAAA,KAAA,EAAA,KAAA,CAAA;AADA;AAHA,iBAAA,CAAA,CAMA,IANA,CAMA,UAAA,QAAA,EAAA;AAAA,kBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,MAAA,EAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AAAA,iBANA,CAhCA;;AAAA;AAwCA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,WAAA;;AAxCA;AAAA;;AAAA;AA2CA,gBAAA,KAAA,CAAA,UAAA,GAAA,IAAA;;AA3CA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA8CA;AAjDA,GAtBA;AA0EA;AACA,EAAA,KAAA,EAAA;AAEA;AACA,IAAA,KAAA,EAAA,iBAAA;AACA,UAAA,EAAA,GAAA,uJAAA;;AACA,UAAA,EAAA,CAAA,IAAA,CAAA,KAAA,KAAA,CAAA,EAAA;AACA,eAAA,KAAA,KAAA,GAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA,KAAA,GAAA,KAAA;AACA;AACA,KAVA;AAYA;AACA,IAAA,QAAA,EAAA,oBAAA;AACA,UAAA,KAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,KAAA,aAAA,GAAA,IAAA;AACA,OAFA,MAEA;AACA,eAAA,KAAA,aAAA,GAAA,KAAA;AACA;AACA;AAnBA;AA3EA,CAAA","sourcesContent":["<template>\r\n  <div id=\"login\">\r\n    <HeaderIcon/>\r\n    <div class=\"box\">\r\n\r\n      <div class=\"top-box\">\r\n        <h1>Registertion</h1>\r\n      </div>\r\n\r\n      <div class=\"buttom-box\">\r\n        <div class=\"form flex\">\r\n          <img src=\"../assets/user.png\" width=\"32px\">\r\n          <input placeholder=\"Username\" type=\"text\" v-model=\"name\" />\r\n        </div>        \r\n        <div class=\"form flex\">\r\n          <img src=\"../assets/mail.png\" width=\"32px\">\r\n          <input placeholder=\"Email\" type=\"email\" v-model=\"email\" />\r\n        </div>\r\n        <div class=\"form flex\">\r\n          <img src=\"../assets/password.png\" width=\"32px\">\r\n          <input placeholder=\"Password\" type=\"password\" v-model=\"password\" />\r\n        </div>\r\n        <button @click=\"auth\">登録</button>\r\n        <p v-if=\"emailError\" class=\"errorLog\">このメールアドレスは有効ではありません。</p>\r\n        <p v-if=\"passwordError\" class=\"errorLog\">パスワードが6文字以下です。</p>\r\n        <p v-if=\"error\" class=\"errorLog\">会員登録に失敗しました</p>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n/** Headerの会社ロゴのコンポーネント */\r\nimport HeaderIcon from '@/components/HeaderIcon.vue'\r\n\r\nimport axios from \"axios\";\r\n\r\nimport firebase from 'firebase';\r\n\r\nexport default {\r\n\r\n\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n      email: \"\",\r\n      password: \"\",\r\n      check:false,\r\n      error:false,\r\n      emailError:false,\r\n      passwordError:false,\r\n      registerAuth:false\r\n    };\r\n  },\r\n\r\n\r\n  components: {\r\n    HeaderIcon\r\n  },\r\n\r\n\r\n  methods: {\r\n\r\n    /** 会員登録の処理 */\r\n    async auth() {\r\n       await firebase.auth().createUserWithEmailAndPassword(this.email, this.password)\r\n      .then(\r\n        // 成功時の処理\r\n        alert('会員登録に成功しました'),\r\n        this.$store.commit('auth',true),\r\n        this.error=false\r\n      )\r\n      .catch(\r\n        error => {\r\n          // エラー時の処理 \r\n          alert(error);\r\n          this.$store.commit('auth',false),\r\n          this.error=true\r\n        }   \r\n      )\r\n\r\n      // firebaseの認証とバリデーションが大丈夫だったらDB側に会員登録処理\r\n      if(this.check===true && this.$store.state.auth===true){\r\n\r\n        // 会員登録api post\r\n        await axios\r\n        .post(this.$store.state.host + \"/api/v1/users/registration\", {\r\n          name: this.name,\r\n          email: this.email,\r\n          password: this.password\r\n        })\r\n        .catch(error => {\r\n          alert(error);\r\n        });\r\n\r\n        // 登録したuser情報を取得するapi Get\r\n        await axios({\r\n              method: \"get\",\r\n              url: this.$store.state.host + \"/api/v1/users\",\r\n              params: {\r\n                email:this.email\r\n              }\r\n        }).then((response) => {this.$store.commit('user',response.data.data[0])})\r\n\r\n        this.$router.push(\"/thankyou\");\r\n      }\r\n      else{\r\n        this.emailError=true;\r\n      }\r\n\r\n    }\r\n  },\r\n\r\n  // 会員登録時のバリデーション\r\n  watch: {\r\n\r\n    // メールのバリデーション\r\n    email: function () {\r\n      var re = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n      if(re.test(this.email)){\r\n        return this.check=true;\r\n      }else{\r\n        return this.check=false;\r\n      }  \r\n    },\r\n\r\n    // パスワードのバリデーション\r\n    password: function(){\r\n      if(this.password.length<7){\r\n        return this.passwordError=true;\r\n      }else{\r\n        return this.passwordError=false;\r\n      }\r\n    }\r\n  },\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n\r\n\r\n#login{\r\n  background-color:rgb(206, 206, 206);\r\n  height:100vh;\r\n  width:100%;\r\n}\r\n\r\n.flex {\r\n  display: flex;\r\n}\r\n\r\n.box {\r\n  display: table;\r\n  margin:200px auto 0;\r\n}\r\n\r\n.top-box{\r\n  width:400px;\r\n  height:25%;\r\n  background-color: #2f00ff;\r\n  padding:20px 20px;\r\n  color:white;\r\n}\r\n\r\n.buttom-box{\r\n  width:440px;\r\n  height:75%;\r\n  padding-top:20px;\r\n  background-color:white;\r\n  color:black;\r\n}\r\n\r\n.form{\r\n  width:80%;\r\n  margin:0 auto 15px;\r\n}\r\n\r\n.form p{\r\n  color:black;\r\n  margin-right:15px;\r\n}\r\n\r\n.form img{\r\n  margin-right:10px;\r\n}\r\n\r\ninput {\r\n  width:80%;\r\n  border:none;\r\n  outline:none;\r\n  color:black;\r\n  border-bottom:2px solid black;\r\n}\r\n\r\n\r\nbutton {\r\n  width: 100px;\r\n  text-align: center;\r\n  padding: 8px 0 10px;\r\n  color: #fff;\r\n  background-color: #2f00ff;\r\n  border-radius: 25px;\r\n  cursor: pointer;\r\n  margin:30px 70% 30px;\r\n}\r\n\r\n.errorLog{\r\n  color: rgb(243, 0, 0);\r\n  text-align: center;\r\n  padding-bottom:30px;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views"}]}